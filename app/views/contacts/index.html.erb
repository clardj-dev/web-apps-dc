<h1>Contacts</h1>

<% contacts = Contact.all %>
<%#= contacts.inspect %>

<ul>
  <% for contact in contacts %>
  <% company = Company.find_by("id" => contact["company_id"])%>
  <li>
    <%= "
    #{contact["first_name"]} 
    #{contact["last_name"]} @ 
    #{company["name"]}, 
    #{contact["email"]}, 
    #{contact["phone_number"]}" 
    %>
  </li>
  <% end %>
<ul/> 